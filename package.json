{
  "name": "bigdator",
  "private": true,
  "babel": {
    "plugins": [
      "transform-object-rest-spread",
      "transform-decorators-legacy"
    ],
    "presets": [
      "es2015",
      "stage0",
      "react"
    ]
  },
  "dependencies": {
    "apollo-client": "0.4.19",
    "apollo-server": "0.3.2",
    "babel-cli": "*",
    "babel-plugin-transform-class-properties": "*",
    "babel-plugin-transform-decorators-legacy": "1.3.4",
    "babel-polyfill": "*",
    "babel-preset-es2015": "*",
    "babel-preset-react": "*",
    "babel-preset-stage-0": "*",
    "body-parser": "1.15.2",
    "bootstrap-sass": "^3.3.6",
    "d3": "4.2.2",
    "express": "4.14.0",
    "flux": "*",
    "graphiql": "0.7.8",
    "graphql": "0.7.0",
    "graphql-tag": "0.1.14",
    "isomorphic-fetch": "*",
    "leaflet": "1.0.1",
    "lodash": "4.15.0",
    "pg": "6.1.0",
    "react": "*",
    "react-apollo": "0.5.6",
    "react-dom": "*",
    "sql": "0.71.0",
    "topojson-client": "2.1.0"
  },
  "devDependencies": {
    "babel-eslint": "*",
    "babel-plugin-transform-decorators-legacy": "1.3.4",
    "babelify": "*",
    "browserify": "*",
    "eslint": "*",
    "eslint-plugin-babel": "*",
    "eslint-plugin-react": "*",
    "exorcist": "*",
    "graphql": "0.7.0",
    "graphql-tools": "0.7.2",
    "minify-geojson": "1.0.8",
    "uglify": "*",
    "watchify": "*"
  },
  "scripts": {
    "start": "babel-node ./server/src/server.js",
    "watch": "watchify ./frontend/js/index.js -t [ babelify ] -o ./frontend/build/app.js --debug --verbose",
    "build": "NODE_ENV=production browserify ./frontend/js/app.js -t [ babelify ] --debug | exorcist ./frontend/build/app.js.map | uglifyjs -c > build/app.js"
  }
}
